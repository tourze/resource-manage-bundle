{
  "name": "tourze/resource-manage-bundle",
  "description": "资源管理",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/dbal": "^3.7.0 || ^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/security-core": "^6.4",
    "tourze/enum-extra": "^0.0.4",
    "psr/log": "^3|^2|^1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\ResourceManageBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\ResourceManageBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}